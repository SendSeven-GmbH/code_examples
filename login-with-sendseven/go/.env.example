# SendSeven OAuth App Credentials
# Get these from your SendSeven dashboard: https://app.sendseven.com/settings/integrations
SENDSEVEN_CLIENT_ID=s7_app_your_client_id_here
SENDSEVEN_CLIENT_SECRET=s7_secret_your_client_secret_here

# SendSeven API URL (default: production)
SENDSEVEN_API_URL=https://api.sendseven.com

# Your application callback URL (must match OAuth app settings)
REDIRECT_URI=http://localhost:3000/callback

# Server port
PORT=3000

# Session encryption key (at least 32 bytes for secure encryption)
# Generate with: openssl rand -base64 32
SESSION_SECRET=your-session-secret-at-least-32-characters-long
